{
    "name": "workflow.io",
    "version": "0.0.14",
    "description": "Data engineering pipeline library for node.js",
    "keywords": [
        "data",
        "pipeline",
        "automation",
        "workflow",
        "data engineering",
        "data migration",
        "data integration",
        "orchestration",
        "ETL",
        "ELT"
    ],
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "module": "dist/index.js",
    "type": "module",
    "private": false,
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/index.js",
            "require": "./dist/index.cjs"
        },
        "./package.json": "./package.json"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/donovanbrun/workflow.git"
    },
    "scripts": {
        "test": "cd test && npm i && npm run dev",
        "dev": "npx tsc -p tsconfig.json && node dist/index.js",
        "build": "tsup",
        "examples": "cd examples && npm i && npm run dev"
    },
    "author": "Donovan Brun",
    "license": "ISC",
    "files": [
        "dist",
        "README.md"
    ],
    "dependencies": {
        "axios": "^0.27.2",
        "csv": "^6.2.8",
        "dotenv": "^16.0.2",
        "express": "^4.18.1",
        "fs": "^0.0.1-security",
        "mongodb": "^5.7.0",
        "node-cron": "^3.0.2",
        "pg": "^8.10.0"
    },
    "devDependencies": {
        "@types/express": "^4.17.17",
        "@types/jsdom": "^21.1.1",
        "@types/node": "^20.4.1",
        "@types/node-cron": "^3.0.8",
        "@types/pg": "^8.11.5",
        "@types/puppeteer": "^7.0.4",
        "@types/uuid": "^9.0.2",
        "ts-node": "^10.9.2",
        "tsup": "^8.0.2",
        "typescript": "^5.3.3"
    }
}